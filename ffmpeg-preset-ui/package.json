{
  "name": "ffmpeg-preset-ui",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "tauri": "tauri",
    "postinstall": "cd ./node_modules/98.css && npm i && npm run build",
    "build:electron": "esbuild src-electron/main.ts --bundle --platform=node --format=cjs --external:electron --outfile=dist-electron/main.cjs --sourcemap --target=node18",
    "build:preload": "esbuild src-electron/preload.ts --bundle --platform=node --format=cjs --external:electron --outfile=dist-electron/preload.cjs --sourcemap --target=node18",
    "watch:electron": "concurrently \"esbuild src-electron/main.ts --bundle --platform=node --format=cjs --external:electron --outfile=dist-electron/main.cjs --sourcemap --target=node18 --watch\" \"esbuild src-electron/preload.ts --bundle --platform=node --format=cjs --external:electron --outfile=dist-electron/preload.cjs --sourcemap --target=node18 --watch\"",
    "electron:run": "pnpm run build:electron && pnpm run build:preload && electron dist-electron/main.cjs",
    "electron:dev": "concurrently \"pnpm dev\" \"pnpm run watch:electron\" \"pnpm run electron:run\""
  },
  "license": "MIT",
  "dependencies": {
    "98.css": "github:jdan/98.css",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-opener": "^2.2.2",
    "@tauri-apps/plugin-shell": "^2",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "solid-fa": "^0.2.0",
    "solid-js": "^1.7.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.3",
    "concurrently": "^8.2.0",
    "electron": "^40.4.1",
    "esbuild": "^0.18.0",
    "sass": "^1.83.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-solid": "^2.8.0"
  }
}
